{
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[markdown]": {
    "editor.autoClosingQuotes": "never",
  },
  "[ruby]": {
    "editor.defaultFormatter": "ruby-syntax-tree.vscode-syntax-tree",
    "editor.formatOnSave": true,
    "editor.semanticHighlighting.enabled": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "breadcrumbs.enabled": false,
  "breadcrumbs.symbolPath": "off",
  "color-highlight.markRuler": false,
  "customWindowZoom.zoomLevelChangeValue": 0.25,
  "cSpell.diagnosticLevel": "Error",
  "cSpell.enabledLanguageIds": [],
  "cSpell.ignorePaths": [
    ".git/objects",
    ".vscode-insiders",
    ".vscode",
    "Gemfile.lock",
    "node_modules",
    "package-lock.json",
    "vscode-extension",
    "yarn.lock"
  ],
  "cSpell.enableFiletypes": [
    "!javascript",
    "!javascriptreact",
    "!json",
    "!less",
    "!ruby",
    "!typescript",
    "!typescriptreact",
    "git-commit",
    "git-rebase",
    "latex",
    "markdown",
    "plaintext"
  ],
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.renderSideBySide": false,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.colorDecorators": false,
  "editor.copyWithSyntaxHighlighting": false,
  "editor.detectIndentation": false,
  "editor.emptySelectionClipboard": false,
  "editor.find.addExtraSpaceOnTop": false,
  "editor.fontFamily": "Monaco",
  "editor.fontSize": 12,
  "editor.hideCursorInOverviewRuler": true,
  "editor.insertSpaces": true,
  "editor.lightbulb.enabled": "off",
  "editor.lineNumbers": "on",
  "editor.maxTokenizationLineLength": 200000,
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.occurrencesHighlight": "off",
  "editor.overviewRulerBorder": false,
  "editor.renderControlCharacters": false,
  "editor.renderLineHighlight":"none",
  "editor.renderWhitespace": "none",
  "editor.rulers": [80, 100, 120],
  "editor.scrollBeyondLastColumn": 0,
  "editor.scrollBeyondLastLine": false,
  "editor.selectionHighlight": true,
  "editor.snippetSuggestions": "top",
  "editor.suggestFontSize": 12,
  "editor.tabSize": 2,
  "editor.unicodeHighlight.invisibleCharacters": false,
  "editor.wordSeparators": "`~!@#%^&*()=+[{]}\\|;:'\",.<>/?",
  "emojisense.languages": {
    "git-commit": true,
    "markdown": true,
    "plaintext": {
      "markupCompletionsEnabled": false,
      "emojiDecoratorsEnabled": false
    },
    "scminput": true,
    "ruby": {
      "emojiDecoratorsEnabled": false
    }
  },
  "emojisense.markupCompletionsEnabled": false,
  "explorer.autoReveal": false,
  "extensions.ignoreRecommendations": true,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.decorations.enabled": true,
  "markdown-preview-github-styles.colorTheme": "light",
  "problems.decorations.enabled": false,
  "scm.diffDecorations": "gutter",
  "search.useGlobalIgnoreFiles": true,
  "search.useParentIgnoreFiles": true,
  "tailwindCSS.colorDecorators": true,
  "tailwindCSS.includeLanguages": {
    "erb": "html"
  },
  "tailwindCSS.experimental.classRegex": [
    "\\bclass:\\s*'([^']*)'",
    "\\bclass:\\s*\"([^\"]*)\""
  ],
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.scrollback": 100000,
  "terminal.integrated.shellIntegration.decorationsEnabled": "never",
  "typescript.updateImportsOnFileMove.enabled": "prompt",
  "vscode_custom_css.imports": ["file:///Users/gray/.dotfiles/vscode/custom.css"],
  "window.newWindowDimensions": "inherit",
  "wordcounter.side.left": [
    "word",
    "char"
  ],
  "wordcounter.languages": [
    "markdown"
  ],
  "workbench.colorCustomizations": {
    // --------- github light default -------
    // slack: #F6F8FA,#F8F8FA,#0969DA,#FFFFFF,#0366D625,#24292F,#1A7F37,#CF222E,#F6F8FA,#24292F
    "editorGutter.addedBackground": "#1a7f37",
    "editorGutter.deletedBackground": "#cf222e",
    "editorGutter.modifiedBackground": "#9a6700",
    "gitDecoration.deletedResourceForeground": "#cf222e",
    "gitDecoration.modifiedResourceForeground": "#9a6700",
    "gitDecoration.untrackedResourceForeground": "#1a7f37",
  },
  "workbench.colorTheme": "GitHub Light Default",
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.list.horizontalScrolling": true,
  "workbench.list.openMode": "doubleClick",
  "workbench.startupEditor": "none",
  "workbench.statusBar.visible": true,
  "workbench.tips.enabled": false,
  "workbench.tree.indent": 20,
  "rubyLsp.enabledFeatures": {
    "codeActions": false,
    "codeLens": false,
    "completion": true,
    "definition": true,
    "diagnostics": true,
    "documentHighlights": true,
    "documentLink": true,
    "documentSymbols": true,
    "foldingRanges": true,
    "formatting": false,
    "hover": true,
    "inlayHint": true,
    "onTypeFormatting": true,
    "selectionRanges": true,
    "semanticHighlighting": true,
    "workspaceSymbol": true
  },
  "prettier.requireConfig": true,
  "editor.inlayHints.enabled": "off",
  "workbench.panel.defaultLocation": "right",
  "window.commandCenter": false,
  "workbench.layoutControl.enabled": false,
  "search.showLineNumbers": true,
  "git.openRepositoryInParentFolders": "never",
  "prettier.useEditorConfig": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "workbench.activityBar.location": "hidden",
  "explorer.compactFolders": false,
  "markdown.preview.scrollPreviewWithEditor": false,
  "markdown.preview.scrollEditorWithPreview": false,
  "window.zoomLevel": -0.25,
  "editor.stickyScroll.enabled": false,
  "rubocop.mode": "enableViaGemfile",
  "rubyLsp.linters": [
    "rubocop"
  ],
  "rubyLsp.rubyVersionManager": {
    "identifier": "rbenv"
  },
  "rubyLsp.featureFlags": {
    "all": true
  },
}
